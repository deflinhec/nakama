    "/v2/account/verify/{token}": {
      "delete": {
        "summary": "Verfiy email address from web ui.",
        "operationId": "Web_VerifyEmailAddress",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
