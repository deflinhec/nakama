  "definitions": {
    "FeaturesFEATURE": {
      "type": "string",
      "enum": [
        "EMAIL_VERIFICATION",
        "VERIFICATION_CODE"
      ],
      "default": "EMAIL_VERIFICATION"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "webFeatures": {
      "type": "object",
      "properties": {
        "features": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/FeaturesFEATURE"
          }
        }
      },
      "description": "Get avaliable features response."
    },
    "webSendEmailVerificationRequest": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "description": "The email address."
        }
      },
      "description": "Submit a email verification request."
    },
    "webSendPasswordResetEmailRequest": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "description": "The email to reset."
        }
      },
      "description": "Submit a password reset request."
    },
    "webVerifyPasswordRenewalRequest": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string",
          "description": "The user credentials."
        },
        "password": {
          "type": "string",
          "description": "The renewed password."
        }
      },
      "description": "Verfiy password after verfied."
    }
  },
