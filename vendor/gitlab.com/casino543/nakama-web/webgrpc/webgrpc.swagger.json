      "post": {
        "summary": "Verfiy password from web ui.",
        "operationId": "Web_VerifyPasswordRenewal",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": "Verfiy password after verfied.",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/webVerifyPasswordRenewalRequest"
            }
          }
        ],
        "tags": [
          "Web"
        ]
      }
    },
    "/v2/account/password/reset": {
